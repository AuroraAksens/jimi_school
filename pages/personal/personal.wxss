
/* 个人信息 */
.m-card{
  width: 100%;
}
.m-card>image{
  width: 100%;
  height: 655rpx;
  position: absolute;
  /* left: -190rpx; */
  top: -135rpx;
  z-index: -1;
}
.m-card>.head>.go{
  width: 50rpx;
  height: 30rpx;
}
.m-card>.head>.go>image{
  width: 30rpx;
  height: 30rpx;
}
.m-card>.brief{
  width: 80%;
  text-align: center;
  color: #888888;
  margin: auto;
  font-size: 27rpx;
}
.m-card>.brief>image{
  width: 40rpx;
  height: 40rpx;
}


/* 任务卡 */
.m-task{
  width: 85%;
  height: 185rpx;
  background-color: white;
  margin: 40rpx auto;
}
.m-task>.body{
  width: 20%;
  height: 150rpx;
  line-height: 60rpx;
  text-align: center;
}

/* 同学 */
.classmate{
  width: 85%;
  background-color: white;
  margin: auto;
}
.classmate>.head>.title{
  font-size: 39rpx;
}
.classmate>.head>.buttom{
  background-color: #FDAE3C;
  color: white;
  font-size: 21rpx;
  text-align: center;
  line-height: 60rpx;
}
.classmate>.body{
  color: #999999;
  font-size: 32rpx;
  text-align: center;
}

.classmate>.content{
  width: 90%;
  margin: 30rpx auto auto auto;
}
.classmate>.content>.box{
  background-color: #F2F2F2;
  color: #3A3939;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  font-size: 24rpx;
}

/* 任务列表 */
.listTask-head{
  width: 90%;
  height: 75rpx;
  margin: 40rpx auto;
  background-color: white;
  border-radius: 50rpx;
}
.listTask-head>.listTitle{
  width: 225rpx;
  height: 75rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 75rpx;
}
.listTask-head>.listTitles{
  width: 225rpx;
  height: 75rpx;
  font-size: 24rpx;
  border-radius: 50rpx;
  background-color: #FDAE3C;
  color: white;
  text-align: center;
  line-height: 75rpx;
}


button::after {
  border: none;
}

button {
  width: 35rpx !important;
  height: 35rpx !important;
  background-color: transparent;
  padding: 0 0 0 0 !important;
  line-height: inherit;
}

.button-hover {
  /*点击后样式*/
  background-color: transparent;
}


.nodata{
  width: 80%;
  height: 400rpx;
  margin: 100rpx auto;
}
