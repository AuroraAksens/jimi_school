/* pages/impression/impression.wxss */
page{
  background-color: white;
}
.head{
  width: 100%;
  margin-top: 50rpx;
}
.head>image{
  width: 115rpx;
  height: 115rpx;
}

.body{
  width: 100%;
}
.body>.top{
  width: 90%;
  margin: 30rpx auto 20rpx auto;
}
.body>.content{
  width: 90%;
  margin: 30rpx auto auto auto;
}
.body>.content>.box{
  background-color: #F2F2F2;
  color: #3A3939;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  font-size: 24rpx;
}

.body>.content>.boxs{
  background-color: #FDAE3C;
  color: white;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  font-size: 24rpx;
}

.body>.bottom{
  width: 90%;
  margin: auto;
}
.body>.bottom>.btn{
  width: 120rpx;
  height: 50rpx;
  border: 1px solid rgba(112,112,112,1);
  border-radius: 50rpx;
  text-align: center;
  font-size:24rpx;
  line-height: 50rpx;
}
.body>.bottom>.btn-add{
  width: 100%;
  height: 50rpx;
  border: 1px solid;
  border-radius: 50rpx;
  overflow: hidden;
  margin: auto;
}
.body>.bottom>.btn-add>.add{
  width: 20%;
  background-color: black;
  color: white;
  text-align: center;
  line-height: 45rpx;
}

.foot{
  width: 90%;
  height: 97rpx;
  text-align: center;
  background-color: #F2F2F2;
  margin: 30rpx auto;
  line-height: 97rpx;
  color: #999999;
}

.foots{
  width: 90%;
  height: 97rpx;
  text-align: center;
  background-color: #FDAE3C;
  margin: 30rpx auto;
  line-height: 97rpx;
  color:white;
}